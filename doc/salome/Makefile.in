# source path
top_srcdir=@top_srcdir@
top_builddir=../..
srcdir=@srcdir@
VPATH=.:@srcdir@

SUBDIRS= tui

@COMMENCE@

dev_docs:
	@@SETX@; for d in $(SUBDIRS); do 	\
	   (cd $$d && $(MAKE) $@) || exit 1; 	\
	done; \

clean:
	@@SETX@; for d in $(SUBDIRS); do 	\
	   if test -d $$d/MED; then		\
	      (cd $$d && $(MAKE) $@) || exit 1;	\
	   fi;                                  \
	done

distclean: clean
	@@SETX@; for d in $(SUBDIRS); do 	\
	   if test -d $$d/MED; then		\
	      (cd $$d && $(MAKE) $@) || exit 1; \
	   fi;                                  \
	done

install:
	@@SETX@; for d in $(SUBDIRS); do 	\
	   if test -d $$d/MED; then		\
	      (cd $$d && $(MAKE) $@); 	        \
	   fi;                                  \
	done

uninstall:
	@@SETX@; for d in $(SUBDIRS); do 	\
	   if test -d $$d/MED; then		\
	      (cd $$d && $(MAKE) $@) || exit 1; \
	   fi;                                  \
	done; \
